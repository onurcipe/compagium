{
    "name": "compagium",
    "version": "0.1.0",
    "description": "A Node.js framework with built-in best practices.",
    "keywords": [
        "compagium",
        "node",
        "node.js",
        "framework"
    ],
    "homepage": "https://github.com/onurcipe/compagium#readme",
    "bugs": {
        "url": "https://github.com/onurcipe/compagium/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Onur Cipe",
        "email": "onur@onurcipe.com",
        "url": "https://onurcipe.com/"
    },
    "funding": {
        "type": "individual",
        "url": "https://onurcipe.com/donate"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onurcipe/compagium.git"
    },
    "scripts": {
        "build": "tsc",
        "clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "mongodb": "^7.0.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.23",
        "@types/node": "^22.19.7",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=22"
    }
}
